{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://front-catalogo-gamma.vercel.app"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "db.json"
    }
  ]
}
